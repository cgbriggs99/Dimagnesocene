fstr = ""
with open("out/magnesium/dimagnesocene+_d5d.xyz", "r") as fp :
    for line in fp :
        newline = line[0]
        for i in range(1, len(line)) :
            if line[i - 1].isupper() and line[i].isupper() :
                newline += line[i].lower()
            else :
                newline += line[i]
        fstr += newline

with open("out/magnesium/dimagnesocene+_d5d.xyz", "w") as fp :
    print(fstr, file = fp)



